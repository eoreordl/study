<body>
  <script>
	  var x = 0;
	  var y = 0;

	  function sumMulti(x,y) {
		if(x === y) {
			let Xsum = x * y;
			return Xsum;
		} 	
		else {
			let sum = x + y;
			return sum;
		}
	}
	sumMulti();
	console.log(sumMulti(10, 11));

  </script>
</body>
</html>
